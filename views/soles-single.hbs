<a href="/">Home</a> > <a href="/soles">Your SOLEs</a> > <strong>{{sole.question.text}}</strong><br>

{{#if sole}}

<h1>{{sole.question.text}}</h1>


<h3>Data from your SOLE session</h3>
<table class="striped responsive-table">
  {{#each sole}}
  <tr>
    <th>{{@key}}</th>
    <td>{{this}}</td>
  </tr>
  {{/each}}

</table>
{{else}}
  <h2>No SOLE found with that ID</h2>
{{/if}}
