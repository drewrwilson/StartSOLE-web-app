<div class="breadcrumb-wrapper">
    <a href="/" class="breadcrumb">Home</a>
    <a href="/admin" class="breadcrumb">Admin</a>
    <a class="breadcrumb">Questions</a>
</div>

<div class="row">
    <div class="col s6">
        <div class="card-panel">
            <h4 class="center-align">Search</h4>
            <div class="input-field">
                <i class="material-icons prefix">search</i>
                <input id="search" name="q" type="search"
                       class="keep-bottom"
                       placeholder="{{{__ "Search by keyword"}}}">
            </div>
            {{>standard-picker}}
        </div>
    </div>
    <div class="col s6">
        <div class="card-panel">
            <h4 class="center-align">Tag</h4>
            {{>standard-picker}}
        </div>
    </div>
    <div class="col s12">
        <div class="card-panel">
            <table class="striped">
                <thead>
                <tr>
                    <th>Select</th>
                    <th>Question</th>
                    <th>Action</th>
                </tr>
                </thead>
                <tbody>

                {{#each questions}}
                    <tr>
                        <td>
                            <label>
                                <input type="checkbox" />
                                <span></span>
                            </label>
                        </td>
                        <td>{{this.text}}</td>
                        <td></td>
                    </tr>
                {{/each}}

                </tbody>
            </table>




        </div>
    </div>
</div>