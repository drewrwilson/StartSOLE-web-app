        function initMap() {
            map = new google.maps.Map(document.getElementById('map'), {
//            center: {lat: 40.0083672, lng: -75.7364713}, //Downingtown, PA
//                 center: {lat: 40.3436036, lng: -82.5155287}, //Ohio
           center: {lat: 39.8283459, lng: -98.5794797}, //US
//                 zoom: 8, //Ohio
               zoom: 3, //US
//                 disableDefaultUI: true,
                styles: styleArray
            });

            layer = new google.maps.FusionTablesLayer({
                query: {
                    select: '\'address\'',
                    from: '{{config.fusionTable}}'
                }
            });
            layer.setMap(map);
        }

        function changeMap() {
            console.log("map change!");
            var whereClause = "'tester' = 'FALSE' ";
            var searchString = document.getElementById('number-of-soles').value.replace(/'/g, "\\'");
            var numSoles = $("#number-of-soles").val();
            var numCEUs = $("#number-of-ceus").val();
            var numFavorites = $("#number-of-favorites").val();
            var startDate = $("#start-date").val();
            var endDate = $("#end-date").val();
            var client = $("#client").val();
            var country = $("#country").val();
            var state = $("#state").val();
            var zip = $("#zip").val();
            var grade = $("#grade").val();
            var subjects = $("#subjects").val();
            var role = $("#role").val();
            if(numSoles>0){
              whereClause += "AND 'soles' >= '" + numSoles + "' ";
            }
            if(numCEUs>0){
              whereClause += "AND 'ceus' >= '" + numCEUs + "' ";
            }
            if(numFavorites>0){
              whereClause += "AND 'favorites' >= '" + numFavorites + "' ";
            }
            if(startDate.length>0){
                whereClause += "AND 'created' >= '" + startDate + "' ";
            }
            if(endDate.length>0){
                whereClause += "AND 'created' <= '" + endDate + "' ";
            }
            if(client && client != "all"){
                whereClause += "AND 'client' = '" + client + "' ";
            }
            if(country && country != "all"){
                whereClause += "AND 'country' = '" + country + "' ";
            }
            if(state && state != "all"){
                whereClause += "AND 'state' = '" + state + "' ";
            }
            if(zip){
                whereClause += "AND 'zip' = '" + zip + "' ";
            }
            if(grade && grade != "all"){
                whereClause += "AND 'grades' CONTAINS IGNORING CASE '" + grade + "' ";
            }
            if(subjects && subjects != "all"){
                whereClause += "AND 'subjects' CONTAINS IGNORING CASE '" + subjects + "' ";
            }
            if(role && role != "all"){
                whereClause += "AND 'role' = '" + role + "' ";
            }


            layer.setOptions({
                query: {
                    select: "col12",
                    from: "{{config.fusionTable}}",
                    where: whereClause
                }
            });
        }

    </script>
